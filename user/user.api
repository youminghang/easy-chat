syntax = "v1"

info (
	title: "用户服务api"
	desc:  "用户服务api内容"
)

type (
	UserReq {
		Id string `form:"id" json:"id"`
	}
	UserResp {
		Id    string `json:"id" form:"id"`
		Name  string `json:"name" form:"name"`
		Phone string `json:"phone" form:"phont"`
	}
)

// 定义user api服务的描述
service User {
	@handler user
	get /user (UserReq) returns (UserResp)
}

